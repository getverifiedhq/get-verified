{
  "name": "get-verified",
  "version": "1.0.0",
  "description": "South Africa's first open-source digital verification platform",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && node dist/main.js",
    "lint": "prettier --write --config prettier.config.js --list-different src/**/*.ts",
    "start": "node ./dist/main.js",
    "test": "tsc && jest --coverage --testPathPattern ./dist/.*\\.test\\.js$"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "onnxruntime-node": "^1.20.1",
    "sharp": "^0.33.5"
  }
}
